{
  "name": "<%= name %>",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "node ./src/server/app.js",
    "start:dev": "nodemon --exec npm run build:dev",
    "build": "webpack --config ./config/webpack.config.js",
    "build:dev":
      "webpack --config ./config/webpack.config.js && node ./src/server/app.js",
    "test": "jest --forceExit"
  },
  "babel": {
    "presets": ["react", "env", "stage-3"]
  },
  "keywords": ["react", "express", "node", "webpack"],
  "author": "<%= author %>",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "chalk": "^2.1.0",
    "css-loader": "^0.28.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "jest-junit": "^3.7.0",
    "morgan": "^1.8.1",
    "node-sass": "^4.9.2",
    "nodemon": "^1.17.5",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.1",
    "webpack-cli": "^2.1.3"
  },
  "jest": {
    "collectCoverage": true,
    "projects": [
      "<rootDir>/src/client/jest.json",
      "<rootDir>/src/server/jest.json"
    ]
  },
  "nodemonConfig": {
    "ext": "js, jsx, scss, css, json",
    "ignore": ["www/", "*.test.js"]
  }
}
